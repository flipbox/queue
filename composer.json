{
  "name": "flipboxdigital/queue",
  "description": "Simple queue",
  "version": "1.0.0-beta.2",
  "type": "craft-plugin",
  "keywords": [
    "queue",
    "flipbox",
    "craftcms",
    "yii2"
  ],
  "require": {
    "php": ">=7.0",
    "craftcms/cms": "~3.0.0-beta.18"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\queue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\queue\\tests\\": "tests/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0",
    "yiisoft/yii2-redis": "*"
  },
  "config": {
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },{
      "type": "path",
      "url": "plugins/asug"
    }
  ],
  "extra": {
    "name": "Queue",
    "handle": "queue",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "class": "flipbox\\queue\\Queue",
    "hasCpSettings": false,
    "hasCpSection": false,
    "schemaVersion": "1.0.0",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
